{
  "name": "react-deployment-s3",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/styled-engine-sc": "^6.4.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "nth-check": "^2.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0",
    "resolve-url-loader": "^5.0.0",
    "styled-components": "^6.1.16",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "react-scripts": "^5.0.1"
  },
  "overrides": {
    "react-scripts": {
      "postcss": ">=8.4.31"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run build && terraform -chdir=terraform/ apply -auto-approve && S3_BUCKET_NAME=$(terraform -chdir=terraform/ output -raw s3_bucket_name) && aws s3 sync ./build \"s3://$S3_BUCKET_NAME\" --delete && terraform -chdir=terraform/ output -raw cloudfront_distribution_url",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
